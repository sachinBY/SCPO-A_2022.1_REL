%dw 2.0
output application/java
---
if(vars.filterCondition != null)
	"select PROJORDERSKUDETAIL.*,ITEM.UOM from PROJORDERSKUDETAIL,ITEM where PROJORDERSKUDETAIL.ITEM = ITEM.ITEM" ++ " AND " ++ vars.filterCondition ++ " ORDER BY PROJORDERSKUDETAIL.ORDERID,PROJORDERSKUDETAIL.ITEM,PROJORDERSKUDETAIL.DEST OFFSET " ++ vars.offset ++ " ROWS FETCH NEXT " ++ vars.next ++ " ROWS ONLY"
else	
	"select PROJORDERSKUDETAIL.*,ITEM.UOM from PROJORDERSKUDETAIL,ITEM where PROJORDERSKUDETAIL.ITEM = ITEM.ITEM" ++ " ORDER BY PROJORDERSKUDETAIL.ORDERID,PROJORDERSKUDETAIL.ITEM,PROJORDERSKUDETAIL.DEST OFFSET " ++ vars.offset ++ " ROWS FETCH NEXT " ++ vars.next ++ " ROWS ONLY"
