%dw 2.0
output application/java
---
if(vars.filterCondition != null)
	"SELECT PROJORDERSUMMARY.*,NETWORKBRACKET.UOM FROM PROJORDERSUMMARY,NETWORKBRACKET WHERE PROJORDERSUMMARY.SOURCE=NETWORKBRACKET.SOURCE and PROJORDERSUMMARY.DEST=NETWORKBRACKET.DEST and PROJORDERSUMMARY.TRANSMODE=NETWORKBRACKET.TRANSMODE" ++ " AND " ++ vars.filterCondition ++ " ORDER BY PROJORDERSUMMARY.ORDERID OFFSET " ++ vars.offset ++ " ROWS FETCH NEXT " ++ vars.next ++ " ROWS ONLY"
else	
	"SELECT PROJORDERSUMMARY.*,NETWORKBRACKET.UOM FROM PROJORDERSUMMARY,NETWORKBRACKET WHERE PROJORDERSUMMARY.SOURCE=NETWORKBRACKET.SOURCE and PROJORDERSUMMARY.DEST=NETWORKBRACKET.DEST and PROJORDERSUMMARY.TRANSMODE=NETWORKBRACKET.TRANSMODE" ++ " ORDER BY PROJORDERSUMMARY.ORDERID OFFSET " ++ vars.offset ++ " ROWS FETCH NEXT " ++ vars.next ++ " ROWS ONLY"
